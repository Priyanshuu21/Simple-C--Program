#include<iostream>
using namespace std;
int main(){
    int num,value,sum=0;
    cout<<"Enter How many number you want to add :";
    cin>>num;
    for(int i=1;i<=num;i++){
        cout<<"Enter your value:";
        cin>>value;
        sum=sum+value;
    }
    cout<<"Your total sum = "<<sum;
    return 0;
}
